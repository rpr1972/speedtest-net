{
  "name": "Speedtest.net",
  "version": "1.0.0",
  "slug": "speedtest-net",
  "description": "Unofficial Speedtest.net integration with Home Assistant via MQTT",
  "url": "https://github.com/rpr1972/speedtest-net",
  "startup": "application",
  "image": "robrosa/speedtest-net",
  "arch": [
    "amd64",
    "aarch64"
  ],
  "host_network": true,
  "map": [
    "config:rw"
  ],
  "options": {
    "MQTT_ADDR": "127.0.0.1",
    "MQTT_USER": "",
    "MQTT_PASS": "",
    "UNIQUE_ID": ""
  },
  "schema": {
    "MQTT_ADDR": "str",
    "MQTT_USER": "str?",
    "MQTT_PASS": "str?",
    "UNIQUE_ID": "str?"
  },
  "environment": {
    "IS_ADDON": "true"
  }
}
